  "bugs": {
    "url": "https://github.com/OzTK/playlist-importer/issues"
  },
  "homepage": "https://github.com/OzTK/playlist-importer#readme",
  "dependencies": {},
  "devDependencies": {
    "elm-live": "^2.7.5"
  }

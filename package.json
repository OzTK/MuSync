{
  "name": "playlist-importer",
  "version": "1.0.0",
  "description": "Import your playlist from and to Spotify, Deezer, Amazon Music, Google Music and so on!",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf elm-stuff & rm -rf tests/elm-stuff",
    "build": "elm-make elm/Main.elm --yes --output=js/elm.js",
    "build:dev": "elm-make elm/Main.elm --yes --output=js/elm.js --debug",
    "debug": "elm-live elm/Main.elm --output=js/elm.js --open --debug",
    "start": "elm-live --output=js/elm.js elm/Main.elm --no-recover",
    "test": "elm-test",
    "cypress": "cypress open",
    "cypress:all": "cypress run",
    "pretest:it": "./test_run.sh",
    "test:it": "cypress run --record --key ${CYPRESS_TOKEN}",
    "posttest:it": "pkill -f elm-live"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OzTK/playlist-importer.git"
  },
  "keywords": [
    "deezer",
    "spotify",
    "amazon",
    "music",
    "google",
    "music",
    "playlist",
    "import",
    "export",
    "importer",
    "exporter",
    "elm"
  ],
  "author": "Paul Duguet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OzTK/playlist-importer/issues"
  },
  "homepage": "https://github.com/OzTK/playlist-importer#readme",
  "dependencies": {},
  "devDependencies": {
    "cypress": "^3.1.0",
    "elm-live": "^3.0.0"
  }
}
